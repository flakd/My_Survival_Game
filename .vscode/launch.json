  "version": "0.2.0",
  "configurations": [
    
  {
    "name": "Launch Edge",
    "request": "launch",
    "type": "msedge",
    //"url": "http://localhost:8080",
    "url": "file:///Volumes/GoogleDrive/My%20Drive/gdDev/js/a_game/map.html",
    "webRoot": "${workspaceFolder}"
  },
   
  {
    "name": "Launch Chrome Local",
    "request": "launch",
    "type": "chrome",
    //"url": "http://localhost:8080",
    "url": "file:///Volumes/GoogleDrive/My%20Drive/gdDev/js/a_game/map.html",
    "webRoot": "${workspaceFolder}"
  },

    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    }
  ]
}